{
  "name": "dtf-editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "db:setup": "node scripts/setup-database.js",
    "prepare": "husky",
    "load:quick": "node load-testing/simple-load-test.js",
    "load:moderate": "CONCURRENT_USERS=25 REQUESTS_PER_USER=15 node load-testing/simple-load-test.js",
    "load:stress": "CONCURRENT_USERS=50 REQUESTS_PER_USER=20 node load-testing/simple-load-test.js",
    "load:flows": "TEST_USERS=10 node load-testing/user-flow-test.js",
    "load:local": "TEST_URL=http://localhost:3000 node load-testing/simple-load-test.js",
    "load:production": "node load-testing/production-test.js",
    "load:production:moderate": "CONCURRENT_USERS=5 REQUESTS_PER_USER=5 node load-testing/production-test.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.1",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-toast": "^1.2.2",
    "@stripe/stripe-js": "^4.10.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.45.4",
    "@types/marked": "^5.0.2",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/functions": "^3.1.0",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "glob": "^11.0.3",
    "lucide-react": "^0.468.0",
    "marked": "^16.3.0",
    "next": "15.4.4",
    "openai": "^5.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.5.2",
    "recharts": "^3.2.1",
    "sharp": "^0.34.3",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^16.6.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "form-data": "^4.0.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "node-fetch": "^2.7.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
