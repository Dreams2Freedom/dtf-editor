{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/webhooks/stripe/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/webhooks/sendgrid/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/generate/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/generate/from-image/route.ts": {
      "maxDuration": 60,
      "maxBodySize": "50mb"
    },
    "src/app/api/generate/image/route.ts": {
      "maxDuration": 60,
      "maxBodySize": "50mb"
    },
    "src/app/api/generate/edit/route.ts": {
      "maxDuration": 60,
      "maxBodySize": "50mb"
    },
    "src/app/api/upload/route.ts": {
      "maxDuration": 60,
      "maxBodySize": "50mb"
    },
    "src/app/api/process/route.ts": {
      "maxDuration": 60,
      "maxBodySize": "50mb"
    },
    "src/app/api/upscale/route.ts": {
      "maxDuration": 60,
      "maxBodySize": "50mb"
    },
    "src/app/api/clippingmagic/upload/route.ts": {
      "maxDuration": 60,
      "maxBodySize": "50mb"
    }
  }
}